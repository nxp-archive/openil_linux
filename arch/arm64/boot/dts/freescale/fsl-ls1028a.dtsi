	gpio0: gpio@2300000 {
		compatible = "fsl,ls1028a-gpio", "fsl,qoriq-gpio";
		reg = <0x0 0x2300000 0x0 0x10000>;
		interrupts = <0 36 0x4>; /* Level high type */
		gpio-controller;
		little-endian;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
	};

	gpio1: gpio@2310000 {
		compatible = "fsl,ls1028a-gpio", "fsl,qoriq-gpio";
