		spi-num-chipselects = <5>;
	};

	usb0:usb3@3100000 {
		compatible= "snps,dwc3";
		reg= <0x0 0x3100000 0x0 0x10000>;
		interrupts= <0 80 0x4>;
		dr_mode= "host";
	};

	usb1:usb3@3110000 {
		compatible= "snps,dwc3";
		reg= <0x0 0x3110000 0x0 0x10000>;
		interrupts= <0 81 0x4>;
		dr_mode= "host";
	};

	sdhci_0: sdhci@02140000 {
		compatible = "fsl,ls1028a-esdhc", "fsl,esdhc";
		reg = <0x0 0x02140000 0x0 0x1000>;
		interrupts = <0 28 0x4>;
		//clocks = <&clock CLK_SDMMC0>, <&clock CLK_SCLK_MMC0>;
		status = "disabled";
	};

	sdhci_1: sdhci@02150000 {
		compatible = "samsung,exynos4210-sdhci";
		reg = <0x0 0x02150000 0x0 0x1000>;
		interrupts = <0 63 0x4>;
		//clocks = <&clock CLK_SDMMC1>, <&clock CLK_SCLK_MMC1>;
		status = "disabled";
	};

	sata@3200000{
		compatible= "fsl,ls1043a-ahci", "fsl,ls1028a-ahci";
		reg= <0X0 0X03200000 0X0 0x1000>;
		interrupts= <0 133 0x4>;
		//clocks= <&gateclk 14>, <&gateclk 20>;/* TODO: add clocks */
		status= "disabled";
	};

	pcie@3400000 {
		compatible = "fsl,ls1028a-pcie";
		reg =  <0x00 0x03400000 0x0 0x100000
			0x80 0x00000000 0x0 0x20000>; /* configuration space */
		reg-names = "regs", "config";
		interrupts = <0 108 0x4>; /* aer interrupt */
		interrupt-names = "aer";
		#address-cells = <3>;
		#size-cells = <2>;
		device_type = "pci";
		dma-coherent;
		num-lanes = <4>;
		bus-range = <0x0 0xff>;
		ranges = <0x81000000 0x0 0x00000000 0x80 0x00020000 0x0 0x00010000   /* downstream I/O */
			  0x82000000 0x0 0x40000000 0x80 0x40000000 0x0 0x40000000>; /* non-prefetchable memory */
		msi-parent = <&its>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0 0 0 7>;
		interrupt-map = <0000 0 0 1 &gic 0 0 0 109 0x4>,
				<0000 0 0 2 &gic 0 0 0 110 0x4>,
				<0000 0 0 3 &gic 0 0 0 111 0x4>,
				<0000 0 0 4 &gic 0 0 0 112 0x4>;
	};

	pcie@3500000 {
		compatible = "fsl,ls1028a-pcie";
		reg =  <0x00 0x03500000 0x0 0x100000
			0x88 0x00000000 0x0 0x20000>; /* configuration space */
		reg-names = "regs", "config";
		interrupts = <0 113 0x4>; /* aer interrupt */
		interrupt-names = "aer";
		#address-cells = <3>;
		#size-cells = <2>;
		device_type = "pci";
		dma-coherent;
		num-lanes = <4>;
		bus-range = <0x0 0xff>;
		ranges = <0x81000000 0x0 0x00000000 0x88 0x00020000 0x0 0x00010000   /* downstream I/O */
			  0x82000000 0x0 0x40000000 0x88 0x40000000 0x0 0x40000000>; /* non-prefetchable memory */
		msi-parent = <&its>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0 0 0 7>;
		interrupt-map = <0000 0 0 1 &gic 0 0 0 114 0x4>,
				<0000 0 0 2 &gic 0 0 0 115 0x4>,
				<0000 0 0 3 &gic 0 0 0 116 0x4>,
				<0000 0 0 4 &gic 0 0 0 117 0x4>;
	};

       rcie_enetc: pcie@1F0000000{         /* rcie_enetc */
               compatible= "pci-host-ecam-generic";
               reg= <0x01 0xF0000000 0x0 0x100000>;
               #address-cells= <3>;
               #size-cells= <2>;
               #interrupt-cells = <1>;
               device_type= "pci";
               bus-range= <0x0 0x0>;
               iommu-map = <0 &smmu 23 3>;
               ranges= <0x82000000 0x0 0x00000000  0x1 0xff000000  0x0 0x100000     /* non-prefetchable memory */
                        0xc2000000 0x0 0x00000000  0x1 0xff100000  0x0 0x100000     /* prefetchable memory */
                        0x82000000 0x0 0x00000000  0x1 0xff200000  0x0 0x100000     /* non-prefetchable memory */
                        0xc2000000 0x0 0x00000000  0x1 0xff300000  0x0 0x100000>;   /* prefetchable memory */
       };


	soc {
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		smmu: iommu@5000000 {
			compatible = "arm,mmu-500";
			reg = <0 0x5000000 0 0x800000>;
			#global-interrupts = <8>;
			#iommu-cells = <1>;
			stream-match-mask = <0x7C00>;
			interrupts = <0 13 4>, /* global secure fault */
				     <0 14 4>, /* combined secure interrupt */
				     <0 15 4>, /* global non-secure fault */
				     <0 16 4>, /* combined non-secure interrupt */
				/* performance counter interrupts 0-7 */
				     <0 211 4>, <0 212 4>,
				     <0 213 4>, <0 214 4>,
				/* per context interrupt, 64 interrupts */
				     <0 146 4>, <0 147 4>,
				     <0 148 4>, <0 149 4>,
				     <0 150 4>, <0 151 4>,
				     <0 152 4>, <0 153 4>,
				     <0 154 4>, <0 155 4>,
				     <0 156 4>, <0 157 4>,
				     <0 158 4>, <0 159 4>,
				     <0 160 4>, <0 161 4>,
				     <0 162 4>, <0 163 4>,
				     <0 164 4>, <0 165 4>,
				     <0 166 4>, <0 167 4>,
				     <0 168 4>, <0 169 4>,
				     <0 170 4>, <0 171 4>,
				     <0 172 4>, <0 173 4>,
				     <0 174 4>, <0 175 4>,
				     <0 176 4>, <0 177 4>,
				     <0 178 4>, <0 179 4>,
				     <0 180 4>, <0 181 4>,
				     <0 182 4>, <0 183 4>,
				     <0 184 4>, <0 185 4>,
				     <0 186 4>, <0 187 4>,
				     <0 188 4>, <0 189 4>,
				     <0 190 4>, <0 191 4>,
				     <0 192 4>, <0 193 4>,
				     <0 194 4>, <0 195 4>,
				     <0 196 4>, <0 197 4>,
				     <0 198 4>, <0 199 4>,
				     <0 200 4>, <0 201 4>,
				     <0 202 4>, <0 203 4>,
				     <0 204 4>, <0 205 4>,
				     <0 206 4>, <0 207 4>,
				     <0 208 4>, <0 209 4>;
		};
	};



	i2c0: i2c@2000000 {
		compatible = "fsl,vf610-i2c";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x0 0x2000000 0x0 0x10000>;
		interrupts = <0 34 0x4>;/* Interrupt lines of i2c1,2 are ORed */
		clock-names = "i2c";
		clocks = <&clockgen 4 3>;
		status = "disabled";
	};

	i2c1: i2c@2010000 {
		compatible = "fsl,vf610-i2c";
