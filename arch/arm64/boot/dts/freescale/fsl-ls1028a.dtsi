	dspi1:dspi@2100000 {
		compatible= "fsl,ls1021a-v1.0-dspi";
		#address-cells= <1>;
		#size-cells= <0>;
		reg = <0x0 0x2100000 0x0 0x10000>;
		interrupts= <0 26 0x4>;
		clocks = <&clockgen 1>;
		clock-names = "dspi";
		spi-num-chipselects = <5>;
	};

	dspi2:dspi@2110000 {
		compatible= "fsl,ls1021a-v1.0-dspi";
		#address-cells= <1>;
		#size-cells= <0>;
		reg= <0x0 0x2110000 0x0 0x1000>;
		interrupts= <0 26 0x4>;
		clocks = <&clockgen 1>;
